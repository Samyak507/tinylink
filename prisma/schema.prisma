generator client {
  provider   = "prisma-client-js"
  engineType = "binary"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {
  id          Int      @id @default(autoincrement())
  code        String   @unique
  url         String
  totalClicks Int      @default(0)
  lastClicked DateTime?
  createdAt   DateTime @default(now())
}
